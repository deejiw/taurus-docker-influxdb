	JSONParser parser = new JSONParser(); 
     Object obj = parser.parse(result);
     JSONObject jsonObj = (JSONObject) obj; 
	
     vars.put(&quot;e2eeSid&quot;, jsonObj.get(&quot;e2eeSid&quot;));
